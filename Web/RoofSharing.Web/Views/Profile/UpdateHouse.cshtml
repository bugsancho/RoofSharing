@model RoofSharing.Web.ViewModels.Profile.HousingViewModel

@{
    ViewBag.Title = "Edit";
}

<h2 class="classic-title">Tell us more about your house!</h2>


@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    @Html.EditorForModel("GenericBootstrapEditorTemplate")
    
    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" value="Save" class="btn btn-warning" />
        </div>
    </div>
}

<div>
    @Html.ActionLink("Back", "Index", "Profile", null, new { @class = "btn btn-info" })
</div>